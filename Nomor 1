#include <iostream>
using namespace std;

int main()
{
int greatest,smallest,number[10];	
for (int i=0;i<10;i++){
	cout<<"Input Number : ";
	cin>>number[i];
	if(i==0){
		greatest=number[0];
		smallest=number[0];
	}
	else if(number[i]>greatest){
		greatest=number[i];
	}
	else if(number[i]<smallest){
		smallest=number[i];
	}
}
cout<<"Greatest = "<<greatest<<endl;
cout<<"Smallest = "<<smallest;
}
